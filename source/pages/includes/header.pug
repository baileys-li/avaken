-
	const logoTitle = `Logotype of ${pageTitle}`
	const navItems = [
				{href: "/", text: "Home"},
				{href: "/", text: "About"},
				{href: "/", text: "Services"},
				{href: "/", text: "News"},
				{href: "/", text: "Contacts"},
		]


header.header
	._wrapper.container
		a(title=logoTitle, aria-label=logoTitle)._logo
			svg(width="149" height="31" viewBox="0 0 149 31" aria-hidden="true" focusable="false" fill="currentColor").logo
				use(href="icons.svg#logo" width="26.85%" height="100%" )._close
				use(href="icons.svg#logotype" x="55" y="7" width="63.09%" height="54.84%")._close

				use(href="icons.svg#logo" width="31.36%" height="100%" )._far
				use(href="icons.svg#logotype" x="64.25" y="8.67" width="56.86%" height="41.97%")._far

		button(type="button" aria-expanded="false" aria-controls="navigation").burger._button
			svg(aria-hidden="true" height=19 width=24 fill="currentColor")._icon
				rect(width=24 height=3)
				rect(width=18 height=3 x=6 y=8)
				rect(width=24 height=3 y=16 )



		nav.navigation._nav#navigation
			ul._list
				each item in navItems
					li
						a(href=item.href)._link=item.text
